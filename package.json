  "main": "server.js",
  "scripts": {
    "client": "webpack-dev-server --optimize-minimize --open --config webpack.dev.js",
    "server": "nodemon --exec babel-node src/server.js --ignore client",
    "start": "concurrently \"yarn server\" \"yarn client\"",
