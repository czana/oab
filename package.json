    "moment": "^2.22.2",
    "node-raspistill": "0.0.15",
    "onoff": "^3.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-toastify": "^4.1.0",
    "redis": "^3.1.1",
    "serialport": "^9.0.7",
    "slack-webhook": "^1.0.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
